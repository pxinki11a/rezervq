<Window x:Class="rezervq.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rezervq"
        mc:Ignorable="d"
        Title="Резервация" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Comic Sans MS"><Run Language="ru-ru" Text="Резервация кабинетов"/></TextBlock>
        <Grid Background="PowderBlue" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="PowderBlue"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" x:Name="MainFrame"></Frame>
        <Button Content="Назад" Name="BtnBack" HorizontalAlignment="Left" Click="BtnBack_Click" Margin="687,21,0,17" Width="113" Height="NaN"></Button>
    </Grid>
</Window>
